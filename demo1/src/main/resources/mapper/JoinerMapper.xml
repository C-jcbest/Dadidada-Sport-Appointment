<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jc.mapper.JoinerMapper">


    <select id="list" resultType="com.jc.entity.pojo.Joiner">
        select * from joiner
    </select>
    <select id="listId" resultType="com.jc.entity.pojo.Joiner">
        select * from joiner where joiner_id = #{joinerId}
    </select>
</mapper>